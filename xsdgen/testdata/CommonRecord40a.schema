<xsd:schema targetNamespace="http://www.ed.gov/FSA/COD/2014/v4.0a" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cod="http://www.ed.gov/FSA/COD/2014/v4.0a" elementFormDefault="qualified">
	<xsd:include schemaLocation="CommonRecord4.0a-VR.xsd"/>
	<xsd:element name="CommonRecord" type="cod:CommonRecordType">
		<xsd:annotation>
			<xsd:documentation>Root Node for Department of Education Common Record</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="CommonRecordType">
		<xsd:sequence>
			<xsd:element name="TransmissionData">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="DocumentID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="CreatedDateTime" type="xsd:dateTime"/>
						<xsd:element name="Source" type="cod:SourceType"/>
						<xsd:element name="Destination" type="cod:DestinationType"/>
						<xsd:element name="Software" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="SoftwareProvider" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="10"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="SoftwareVersion" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="6"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="FullResponseCode" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:length value="1"/>
									<xsd:enumeration value="S"/>
									<xsd:enumeration value="F"/>
									<xsd:enumeration value="M"/>
									<xsd:enumeration value="N"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ReportingSchool" type="cod:ReportingSchoolType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Receipt" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="Response" type="cod:CommonRecordResponseType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CommonRecordResponseType">
		<xsd:sequence>
			<xsd:element name="DocumentTypeCode" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="2"/>
						<xsd:maxLength value="2"/>
						<xsd:enumeration value="BN"/>
						<xsd:enumeration value="PS"/>
						<xsd:enumeration value="PN"/>
						<xsd:enumeration value="ND"/>
						<xsd:enumeration value="RC"/>
						<xsd:enumeration value="RS"/>
						<xsd:enumeration value="SG"/>
						<xsd:enumeration value="WB"/>
						<xsd:enumeration value="CO"/>
						<xsd:enumeration value="AT"/>
						<xsd:enumeration value="AC"/>
						<xsd:enumeration value="SP"/>
						<xsd:enumeration value="EC"/>
						<xsd:enumeration value="SU"/>
						<xsd:enumeration value="PC"/>
						<xsd:enumeration value="OF"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DocumentStatusCode" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="1"/>
						<xsd:enumeration value="A"/>
						<xsd:enumeration value="R"/>
						<xsd:enumeration value="D"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ProcessDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="EditProcessResult" type="cod:EditProcessResultType" minOccurs="0" maxOccurs="100"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EntityRefType">
		<xsd:sequence>
			<xsd:element name="RoutingID">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="8"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AttendedSchoolType" mixed="false">
		<xsd:complexContent mixed="false">
			<xsd:extension base="cod:EntityRefType">
				<xsd:sequence>
					<xsd:element name="Student" type="cod:StudentType" minOccurs="0" maxOccurs="unbounded">
						<xsd:key name="LIK">
							<xsd:selector xpath="LoanInformation"/>
							<xsd:field xpath="@AwardKey"/>
						</xsd:key>
						<xsd:keyref name="LIKRef" refer="cod:LIK">
							<xsd:selector xpath="FinancialAward"/>
							<xsd:field xpath="AwardKey"/>
						</xsd:keyref>
					</xsd:element>
					<xsd:element name="Response" type="cod:ResponseType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ReportingSchoolType" mixed="false">
		<xsd:complexContent mixed="false">
			<xsd:extension base="cod:EntityRefType">
				<xsd:sequence>
					<xsd:element name="ReportedFinancialSummary" type="cod:ReportedFinancialSummaryType" minOccurs="0" maxOccurs="120"/>
					<xsd:element name="AttendedSchool" type="cod:AttendedSchoolType" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="Response" type="cod:ReportingSchoolResponseType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ReportedFinancialSummaryType">
		<xsd:sequence>
			<xsd:element name="FinancialAwardType" type="cod:FinancialAwards"/>
			<xsd:element name="FinancialAwardYear" type="xsd:gYear"/>
			<xsd:element name="TotalCount" type="cod:TotalCountType"/>
			<xsd:element name="TotalReportedAward" type="cod:LargeCurrencyType" minOccurs="0"/>
			<xsd:element name="TotalReportedDisbursement" type="cod:LargeCurrencyType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DestinationType">
		<xsd:choice>
			<xsd:element name="School" type="cod:EntityRefType"/>
			<xsd:element name="ThirdPartyServicer" type="cod:EntityRefType"/>
			<xsd:element name="Guarantor" type="cod:EntityRefType"/>
			<xsd:element name="Lender" type="cod:EntityRefType"/>
			<xsd:element name="Other" type="cod:EntityRefType"/>
			<xsd:element name="COD" type="cod:EntityRefType"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="SourceType">
		<xsd:choice>
			<xsd:element name="School" type="cod:EntityRefType"/>
			<xsd:element name="ThirdPartyServicer" type="cod:EntityRefType"/>
			<xsd:element name="Guarantor" type="cod:EntityRefType"/>
			<xsd:element name="Lender" type="cod:EntityRefType"/>
			<xsd:element name="Other" type="cod:EntityRefType"/>
			<xsd:element name="COD" type="cod:EntityRefType"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="PersonType">
		<xsd:sequence>
			<xsd:element name="Index">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="SSN">
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="cod:SSNType"/>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="BirthDate" type="xsd:date"/>
						<xsd:element name="LastName" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="0"/>
									<xsd:maxLength value="35"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="PersonIdentifiers" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="SSN" minOccurs="0">
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="cod:SSNType"/>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="DriversLicense" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="DriversLicenseState" type="cod:StateProvinceCodeType"/>
									<xsd:element name="DriversLicenseNumber">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="20"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="SchoolAssignedPersonID" nillable="true" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Birth" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="BirthDate" type="xsd:date" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Name" type="cod:NameType" minOccurs="0"/>
			<xsd:element name="Contacts" type="cod:ContactsType" minOccurs="0"/>
			<xsd:element name="Citizenship" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="CitizenshipStatusCode" nillable="true" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:length value="1"/>
									<xsd:enumeration value="1"/>
									<xsd:enumeration value="2"/>
									<xsd:enumeration value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Note" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="cod:SchoolNoteMessage" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BorrowerType" mixed="false">
		<xsd:complexContent mixed="false">
			<xsd:extension base="cod:PersonType">
				<xsd:sequence>
					<xsd:element name="DefaultOverpayCode" type="cod:DefaultOverpayCodeType" minOccurs="0"/>
					<xsd:element name="Response" type="cod:ResponseType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="StudentType" mixed="false">
		<xsd:complexContent mixed="false">
			<xsd:extension base="cod:PersonType">
				<xsd:sequence>
					<xsd:element ref="cod:LoanInformation" minOccurs="0" maxOccurs="60"/>
					<xsd:element ref="cod:FinancialAward" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="cod:Counseling" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="Response" type="cod:StudentResponseType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EndorserType" mixed="false">
		<xsd:sequence>
			<xsd:element name="Index">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="SSN">
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="cod:SSNType"/>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="BirthDate" type="xsd:date"/>
						<xsd:element name="LastName" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="0"/>
									<xsd:maxLength value="35"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Name" type="cod:EndorserNameType" minOccurs="0"/>
			<xsd:element name="DriversLicense" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="DriversLicenseState" type="cod:StateProvinceCodeType" minOccurs="0"/>
						<xsd:element name="DriversLicenseNumber" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Contacts" type="cod:ContactsType" minOccurs="0"/>
			<xsd:element name="Citizenship" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="CitizenshipStatusCode" nillable="true" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:length value="1"/>
									<xsd:enumeration value="1"/>
									<xsd:enumeration value="2"/>
									<xsd:enumeration value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="LoanInformation" type="cod:LoanInformationType" abstract="true"/>
	<xsd:element name="DLLoanInformation" type="cod:DLLoanInformationType" substitutionGroup="cod:LoanInformation"/>
	<xsd:complexType name="LoanInformationType">
		<xsd:sequence>
			<xsd:element name="OriginationFeePercent" nillable="true" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="100"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="InterestRebatePercent" nillable="true" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="100"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PromissoryNotePrintCode" nillable="true" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="1"/>
						<xsd:enumeration value="S"/>
						<xsd:enumeration value="R"/>
						<xsd:enumeration value="Z"/>
						<xsd:enumeration value="V"/>
						<xsd:enumeration value="O"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DisclosureStatementPrintCode" nillable="true" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="1"/>
						<xsd:enumeration value="Y"/>
						<xsd:enumeration value="R"/>
						<xsd:enumeration value="S"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element ref="cod:StudentLevelCode" minOccurs="0"/>
			<xsd:element name="FinancialAwardBeginDate" type="xsd:date" nillable="true" minOccurs="0"/>
			<xsd:element name="FinancialAwardEndDate" type="xsd:date" nillable="true" minOccurs="0"/>
			<xsd:element name="AcademicYearBeginDate" type="xsd:date" nillable="true" minOccurs="0"/>
			<xsd:element name="AcademicYearEndDate" type="xsd:date" nillable="true" minOccurs="0"/>
			<xsd:element name="PublishedPgmLengthYears" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="999.999"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PublishedPgmLengthMonths" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="999.999"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PublishedPgmLengthWeeks" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="999.999"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="WeeksProgramsAcademicYear" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="999.999"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="SpecialPrograms" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ProgramCredentialLevel" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="AwardKey" type="xsd:integer" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="DLLoanInformationType" mixed="false">
		<xsd:complexContent mixed="false">
			<xsd:extension base="cod:LoanInformationType">
				<xsd:sequence>
					<xsd:element name="Response" type="cod:ResponseType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="FinancialAward" type="cod:FinancialAwardType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>At this location in your document you can include one or more FinancialAwardType elements (ex: DLPLUS, DLSubsidized, Pell, DLUnsubsidized, ACG, SMART, and Grad-PLUS)  </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Pell" type="cod:PellType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="DLSubsidized" type="cod:DLSubsidizedType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="DLUnsubsidized" type="cod:DLUnsubsidizedType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="DLPLUS" type="cod:DLPLUSType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="Perkins" type="cod:PerkinsType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="SEOG" type="cod:SEOGType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="FWSP" type="cod:FWSPType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="AlternativeLoan" type="cod:AlternativeLoanType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="AcademicCompetitivenessGrant" type="cod:AcademicCompetitivenessGrantType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="NationalSMARTGrant" type="cod:NationalSMARTGrantType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="DLGraduatePLUS" type="cod:DLGraduatePLUSType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="TEACH" type="cod:TEACHType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="Counseling" type="cod:CounselingType"/>
	<xsd:complexType name="FinancialAwardType">
		<xsd:sequence>
			<xsd:element name="AwardKey" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="FinancialAwardYear" type="xsd:gYear"/>
			<xsd:element name="CPSTransactionNumber" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="FinancialAwardAmount" type="cod:SmallCurrencyType" nillable="true" minOccurs="0"/>
			<xsd:element name="Note" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="cod:SchoolNoteMessage" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CampusBasedAwardType" mixed="false">
		<xsd:complexContent mixed="false">
			<xsd:extension base="cod:FinancialAwardType">
				<xsd:sequence>
					<xsd:element name="DependencyStatusCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="I"/>
								<xsd:enumeration value="D"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="LessThanFullTimeIndicator" type="xsd:boolean" nillable="true" minOccurs="0"/>
					<xsd:element name="FederalShareAmount" type="cod:SmallCurrencyType" nillable="true" minOccurs="0"/>
					<xsd:element ref="cod:StudentLevelCode" minOccurs="0"/>
					<xsd:element name="FISAPIncomeAmount" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:decimal">
								<xsd:minInclusive value="-99999.99"/>
								<xsd:maxInclusive value="999999.99"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="SecondaryEFCCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:length value="1"/>
								<xsd:enumeration value="O"/>
								<xsd:enumeration value="S"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="Response" type="cod:ResponseType" minOccurs="0"/>
					<xsd:element name="Disbursement" minOccurs="0" maxOccurs="52">
						<xsd:complexType mixed="false">
							<xsd:complexContent mixed="false">
								<xsd:extension base="cod:DisbursementType">
									<xsd:sequence>
										<xsd:element name="Response" type="cod:ResponseType" minOccurs="0"/>
									</xsd:sequence>
								</xsd:extension>
							</xsd:complexContent>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
